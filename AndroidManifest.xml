<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mycarwiki"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.mycarwiki.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.mycarwiki.MeuCarroActivity"
            android:label="@string/title_activity_meu_carro" >
        </activity>
        <activity
            android:name="com.example.mycarwiki.ManutencaoPeriodicaActivity"
            android:label="@string/title_activity_manutencao_periodica" >
        </activity>
        <activity
            android:name="com.example.mycarwiki.ComparativoActivity"
            android:label="@string/title_activity_comparativo" >
        </activity>
        <activity
            android:name="com.example.mycarwiki.AtualizaKmActivity"
            android:label="@string/title_activity_atualiza_km" >
        </activity>
    </application>

</manifest>
